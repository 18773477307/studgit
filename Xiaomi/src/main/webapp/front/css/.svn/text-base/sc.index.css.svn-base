.goods_list_box{
	height:1392px;
	width:1240px;
	margin:0;
}
.goods_list{
	height:1347px;
	width:1240px;
	margin:0;
}
.goods_item {
    background: #fff none repeat scroll 0 0;
    float: left;
    height: 383px;
    margin-bottom: 14px;
    margin-right: 14px;
    padding-top: 47px;
    position: relative;
    text-align: center;
    transition: box-shadow 0.5s linear 0s;
    width: 296px;
}
.goods_item:hover {
	box-shadow:0 5px 15px #ccc;
	transform:translate3d(0px,-2px,0px);
}
.goods_item .actions .btn_like {
    left: 20px;
}
.goods_item .actions .btn_buy {
    right: 20px;
}
.goods_item .actions .btn_like, .goods_item .actions .btn_buy {
    color: #424242;
    font-size: 12px;
    height: 24px;
    opacity: 0;
    padding: 3px 4px;
    position: absolute;
    top: 20px;
    transition: all 0.2s linear 0s;
    z-index: 3;
}
a {
    color: #757575;
    text-decoration: none;
}
.goods_item .actions .btn_like:hover, .goods_item .actions .btn_buy:hover{
	color:#424242;
}
.goods_item:hover .actions .btn_like, .goods_item:hover .actions .btn_buy{
	opacity:1;
}

.goods_item .actions img {
    width: 24px;
    height: 29px;
}
.goods_item .actions .btn_like:hover span, .goods_item .actions .btn_buy:hover span{
	opacity:1;
}
.goods_item .actions span {
    opacity: 0;
    transition: opacity 0.2s linear 0s;
}
.goods_item .figure_img{
	width:200px;
	height:200px;	
}
.goods_item .desc {
    color: #b0b0b0;
    font-size: 12px;
    height: 18px;
    margin: 0 0 10px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
p, pre {
    margin: 14px 0;
}
.goods_item .title {
    font-size: 14px;
    font-weight: 400;
    margin: 0 0 3px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
h2 {
    font-size: 21px;
    margin: 11.63px 0;
}
.goods_item .price{
	color:#ff6700;
	margin:0 0 15px;
}
.goods_item .flags{
	 height: 20px;
     margin-top:38px;
	 background-color: #e53935;
}
.goods_item .flags .flags_sale {
    color: #fff;
    height: 18px;
	font-size:12px;
	text-align:left;
    padding: 1px 20px;
}
.goods_page{
	height:30px;
	padding:30px 0;
	text-align:center;
}
.goods_page a{
    color: #b0b0b0;
    font-size: 18px;
    height: 24px;
	width: 48px;
    line-height: 24px;
    margin: 0 8px;
    padding: 3px 0;
	font-weight:bold;
	display:inline-block;
	cursor:pointer;
}
.goods_page a:hover{
	background:#999;
	color:#fff;
}





