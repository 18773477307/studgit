package com.xiaomi.entity;

public class GoodsAppr {
	private String apprId;
	private String goodsId;
	private String usersId;
	private String apprCont;
	private String apprDate;
	private int apprSta;
	private String reserve13;
	private String reserve14;
	
	private String usersName;//用户名
	private String goodsName;//商品名
	
	public String getUsersName() {
		return usersName;
	}
	public String getGoodsName() {
		return goodsName;
	}
	public void setUsersName(String usersName){
		this.usersName=usersName;
	}
	public void setGoodsName(String goodsName){
		this.goodsName=goodsName;
	}
	
	//评价编号
	public String getApprId() {
		return apprId;
	}
	public String getApprIds() {
		return apprId;
	}
	
	public void setApprId(String apprId) {
		this.apprId = apprId;
	}
	
	//商品编号，引入外键
	public String getGoodsId() {
		return goodsId;
	}
	
	public void setGoodsId(String goodsId) {
		this.goodsId = goodsId;
	}
	
	//用户编号 引入外键
	public String getUsersId() {
		return usersId;
	}
	
	public void setUsersId(String usersId) {
		this.usersId = usersId;
	}
	
	//评价内容
	public String getApprCont() {
		return apprCont;
	}
	
	public void setApprCont(String apprCont) {
		this.apprCont = apprCont;
	}
	
	//评价时间
	public String getApprDate() {
		return apprDate;
	}
	
	public void setApprDate(String apprDate) {
		this.apprDate = apprDate;
	}
	
	//评价状态
	public int getApprSta() {
		return apprSta;
	}
	
	public void setApprSta(int apprSta) {
		this.apprSta = apprSta;
	}
	
	//备用字段
	public String getReserve13() {
		return reserve13;
	}
	
	public void setReserve13(String reserve13) {
		this.reserve13 = reserve13;
	}
	
	public String getReserve14() {
		return reserve14;
	}
	
	public void setReserve14(String reserve14) {
		this.reserve14 = reserve14;
	}

	public GoodsAppr(String apprId, String goodsId, String usersId,
			String apprCont, String apprDate, int apprSta, String reserve13,
			String reserve14) {
		super();
		this.apprId = apprId;
		this.goodsId = goodsId;
		this.usersId = usersId;
		this.apprCont = apprCont;
		this.apprDate = apprDate;
		this.apprSta = apprSta;
		this.reserve13 = reserve13;
		this.reserve14 = reserve14;
	}

	public GoodsAppr() {
		super();
	}

	@Override
	public String toString() {
		return "GoodsAppr [apprId=" + apprId + ", goodsId=" + goodsId
				+ ", usersId=" + usersId + ", apprCont=" + apprCont
				+ ", apprDate=" + apprDate + ", apprSta=" + apprSta
				+ ", reserve13=" + reserve13 + ", reserve14=" + reserve14 + "]";
	}
	
	
}
