package com.xiaomi.entity;

public class UsersInfo {
	private String usersId;
	private String usersName;
	private String usersTel;
	private String usersEmail;
	private String usersPwd;
	private int usersSex;
	private String usersIDCard;
	private int usersAge;
	private String usersPhoto;
	private String balance;
	private int usersSta;
	private String reserve1;
	private String reserve2;
	
	
	//用户编号
	public String getUsersId() {
		return usersId;
	}
	
	public String getUsersIds() {
		return usersId;
	}
	
	public void setUsersId(String usersId) {
		this.usersId = usersId;
	}
	
	//用户姓名
	public String getUsersName() {
		return usersName;
	}
	
	public void setUsersName(String usersName) {
		this.usersName = usersName;
	}
	
	//用户电话
	public String getUsersTel() {
		return usersTel;
	}
	
	public void setUsersTel(String usersTel) {
		this.usersTel = usersTel;
	}
	
	//用户邮箱
	public String getUsersEmail() {
		return usersEmail;
	}
	
	public void setUsersEmail(String usersEmail) {
		this.usersEmail = usersEmail;
	}
	
	//用户密码
	public String getUsersPwd() {
		return usersPwd;
	}
	
	public void setUsersPwd(String usersPwd) {
		this.usersPwd = usersPwd;
	}
	
	//用户性别
	public int getUsersSex() {
		return usersSex;
	}

	public void setUsersSex(int usersSex) {
		this.usersSex = usersSex;
	}
	
	//用户身份证号码
	public String getUsersIDCard() {
		return usersIDCard;
	}

	public void setUsersIDCard(String usersIDCard) {
		this.usersIDCard = usersIDCard;
	}
	
	//用户年龄
	public int getUsersAge() {
		return usersAge;
	}
	
	public void setUsersAge(int usersAge) {
		this.usersAge = usersAge;
	}
	
	//用户头像
	public String getUsersPhoto() {
		return usersPhoto;
	}
	
	public void setUsersPhoto(String usersPhoto) {
		this.usersPhoto = usersPhoto;
	}
	
	//用户余额
	public String getBalance() {
		return balance;
	}
	
	public void setBalance(String balance) {
		this.balance = balance;
	}
	
	//用户状态
	public int getUsersSta() {
		return usersSta;
	}
	
	public void setUsersSta(int usersSta) {
		this.usersSta = usersSta;
	}
	
	//备用字段
	public String getReserve1() {
		return reserve1;
	}
	
	public void setReserve1(String reserve1) {
		this.reserve1 = reserve1;
	}
	
	public String getReserve2() {
		return reserve2;
	}
	
	public void setReserve2(String reserve2) {
		this.reserve2 = reserve2;
	}
	
	public UsersInfo() {
		super();
	}

	public UsersInfo(String usersId, String usersName, String usersTel,
			String usersEmail, String usersPwd, int usersSex,
			String usersIDCard, int usersAge, String usersPhoto,
			String balance, int usersSta, String reserve1, String reserve2) {
		super();
		this.usersId = usersId;
		this.usersName = usersName;
		this.usersTel = usersTel;
		this.usersEmail = usersEmail;
		this.usersPwd = usersPwd;
		this.usersSex = usersSex;
		this.usersIDCard = usersIDCard;
		this.usersAge = usersAge;
		this.usersPhoto = usersPhoto;
		this.balance = balance;
		this.usersSta = usersSta;
		this.reserve1 = reserve1;
		this.reserve2 = reserve2;
	}

	public UsersInfo(String usersName, String usersTel, String usersEmail,
			String usersPwd, int usersSex, String usersIDCard) {
		super();
		this.usersName = usersName;
		this.usersTel = usersTel;
		this.usersEmail = usersEmail;
		this.usersPwd = usersPwd;
		this.usersSex = usersSex;
		this.usersIDCard = usersIDCard;
	}
	
	public UsersInfo(String usersId, String usersName, String usersTel, String usersEmail,
			String usersPwd, int usersSex, String usersIDCard) {
		super();
		this.usersId = usersId;
		this.usersName = usersName;
		this.usersTel = usersTel;
		this.usersEmail = usersEmail;
		this.usersPwd = usersPwd;
		this.usersSex = usersSex;
		this.usersIDCard = usersIDCard;
	}

	@Override
	public String toString() {
		return "UsersInfo [usersId=" + usersId + ", usersName=" + usersName
				+ ", usersTel=" + usersTel + ", usersEmail=" + usersEmail
				+ ", usersPwd=" + usersPwd + ", usersSex=" + usersSex
				+ ", usersIDCard=" + usersIDCard + ", usersAge=" + usersAge
				+ ", usersPhoto=" + usersPhoto + ", balance=" + balance
				+ ", usersSta=" + usersSta + ", reserve1=" + reserve1
				+ ", reserve2=" + reserve2 + "]";
	}
	
	
}
